  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alanrsoares/u-semver/issues"
  },
  "homepage": "https://github.com/alanrsoares/u-semver#readme",
  "devDependencies": {
    "babel": "^5.6.23",
    "babel-core": "^5.7.2",
    "babel-eslint": "^3.1.23",
    "babel-loader": "^5.3.2",
    "chai": "^3.0.0",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "uglify-js": "^2.4.23",
    "webpack": "^1.10.1"
  }
